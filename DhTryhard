https://raw.githubusercontent.com/mezzopera/Omen-Hub/main/omen_hub.lua%22))()
wait (1)
while true do
    local character = game.Players.LocalPlayer.Character
    character.Animate.walk.WalkAnim.AnimationId = "http://www.roblox.com/asset/?id=616168032"
    character.Animate.run.RunAnim.AnimationId = "http://www.roblox.com/asset/?id=616163682"
    character.Animate.jump.JumpAnim.AnimationId = "http://www.roblox.com/asset/?id=656117878"
    character.Animate.fall.FallAnim.AnimationId = "http://www.roblox.com/asset/?id=656115606"
    character:FindFirstChild("Head").Transparency = 1
    character:FindFirstChild("RightFoot").Transparency = 1
    character:FindFirstChild("RightLowerLeg").Transparency = 1
    character:FindFirstChild("RightUpperLeg").Transparency = 1
    wait(1)
end
